<!DOCTYPE html>
<html>
<head>
  <title>BlogHub</title>
  <%= stylesheet_link_tag    'application', 'https://fonts.googleapis.com/css?family=Chewy|Architects+Daughter|Indie+Flower|Ravi+Prakash', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= render 'layouts/navigation' %>
  <%= render 'layouts/messages' %>
  <%= yield %>
  <%= render 'layouts/footer' %>
  <%= debug(params) if Rails.env.development? %>
</body>
</html>
